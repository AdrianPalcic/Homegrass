{
    "version": 2,
    "routes": [
        {
            "src": "/sitemap.xml",
            "headers": {
                "Content-Type": "application/xml",
                "Cache-Control": "public, max-age=3600"
            }
        },
        {
            "src": "/cms/wp-admin/(.*)",
            "dest": "https://artificialgrass.hr/homegrass.hr/cms/wp-admin/$1",
            "status": 200
        },
        {
            "src": "/cms/(.*)",
            "dest": "https://artificialgrass.hr/homegrass.hr/cms/$1",
            "status": 200
        },
        {
            "src": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "www.homegrass.hr"
                }
            ],
            "dest": "https://homegrass.hr/$1",
            "status": 301
        },
        {
            "src": "/((?!api/).*)",
            "dest": "/index.html",
            "status": 200
        }
    ]
}