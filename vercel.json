{
    "rewrites": [
        {
            "source": "/cms/wp-admin/:path*",
            "destination": "https://artificialgrass.hr/homegrass.hr/cms/wp-admin/:path*"
        },
        {
            "source": "/cms/:path*",
            "destination": "https://artificialgrass.hr/homegrass.hr/cms/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "www.homegrass.hr",
            "destination": "https://homegrass.hr",
            "permanent": true
        }
    ]
}