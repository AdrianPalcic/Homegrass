{
    "version": 2,
    "routes": [
        {
            "src": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "www.homegrass.hr"
                }
            ],
            "dest": "https://homegrass.hr/:path*",
            "status": 301
        },
        {
            "src": "/cms/wp-admin/:path*",
            "dest": "https://artificialgrass.hr/homegrass.hr/cms/wp-admin/:path*",
            "status": 200
        },
        {
            "src": "/cms/:path*",
            "dest": "https://artificialgrass.hr/homegrass.hr/cms/:path*",
            "status": 200
        },
        {
            "src": "/:path*",
            "dest": "/index.html",
            "status": 200
        }
    ],
    "headers": [
        {
            "source": "/:path*",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "index, follow"
                },
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=63072000; includeSubDomains; preload"
                }
            ]
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}