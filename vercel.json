{
  "version": 2,
  "rewrites": [
    {
      "source": "/cms/wp-admin/(.*)",
      "destination": "https://artificialgrass.hr/homegrass.hr/cms/wp-admin/$1"
    },
    {
      "source": "/cms/(.*)",
      "destination": "https://artificialgrass.hr/homegrass.hr/cms/$1"
    },
    {
      "source": "/((?!cms|sitemap\\.xml|robots\\.txt|favicon\\.ico|static|.*\\.js|.*\\.css).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Refresh",
          "value": ""
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.homegrass.hr"
        }
      ],
      "destination": "https://homegrass.hr/:path*",
      "permanent": true
    }
  ]
}
