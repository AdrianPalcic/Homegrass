{
    "version": 2,
    "routes": [
        {
            "src": "/sitemap.xml",
            "headers": {
                "Content-Type": "application/xml",
                "Cache-Control": "public, max-age=3600"
            }
        },
        {
            "src": "/wp-admin/:path*",
            "dest": "https://artificialgrass.hr/homegrass.hr/cms/wp-admin/:path*",
            "status": 200
        },
        {
            "src": "/cms/:path*",
            "dest": "https://artificialgrass.hr/homegrass.hr/cms/:path*",
            "status": 200
        },
        {
            "src": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "www.homegrass.hr"
                }
            ],
            "dest": "https://homegrass.hr/:path*",
            "status": 301
        },
        {
            "src": "/:path*",
            "dest": "/index.html",
            "status": 200
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}